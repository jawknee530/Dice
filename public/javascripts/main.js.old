$(function(){
  $('.roll').on('keyup', function(ev){
    if(ev.keyCode === 13) {
      //console.log('Keyup on enter working')
      var params = {
        roll: $(this).val()
      };
      console.log(params)
      $.get('/roll', params, function(data) {
        $('.results').html(data);
      });
    };
  });
});
